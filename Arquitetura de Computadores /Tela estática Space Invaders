.text
main:
	jal cores
	jal def_fundo
	jal monstrinhos
	jal nave
	#jal mover_nave
	#jal mover_monstrinhos
	#jal atirar_nave
	
	j fim
	
	
cores:	
	addi $20, $0, 0x596969 #onde tá o preto
	addi $21, $0, 0x00F7F7 #onde tá o azul
	addi $22, $0, 0xFFFFFF #onde tá o branco
	jr $31


def_fundo:
	addi $9, $0, 8192 #quantidade de pontos exibidos na tela (512 * 256) / 16
	add $10, $0, $9 #mapeamento
	lui $10, 0x1001 #endereço inicial
	jr $31
	
	
fundo:	
	beq $9, $0, def_fundo
	sw $20, 0($10) #exibir o preto$20 na posição 0
	addi $10, $10, 4 #pulo
	addi $9, $9, -1 #redução L -1
	j fundo
	
monstrinhos:
